GET vietnamworks/job/_search?search_type=count
{
  "query": {
    "filtered": {
      "query": {
        "bool": {
          "should": [
            {
              "match_phrase": {
                "jobTitle": "java"
              }
            },
            {
              "match_phrase": {
                "jobTitle": "j2ee"
              }
            },
            {
              "match_phrase": {
                "jobTitle": "java ee"
              }
            }
          ]
        }
      },
      "filter": {
        "term": {
          "jobLevelId": 5
        }
      }
    }
  },
  "aggs": {
    "avg_salary_min": {
      "filter": {
        "and": {
          "filters": [
            {
              "range": {
                "salaryMin": {
                  "from": 250
                }
              }
            },
            {
              "range": {
                "salaryMax": {
                  "from": 250
                }
              }
            },
            {
              "range": {
                "expiredDate": {
                  "from": "now"
                }
              }
            }
          ]
        }
      },
      "aggs": {
        "avg_salary_min": {
          "avg": {
            "field": "salaryMin"
          }
        }
      }
    },
    "avg_salary_max": {
      "filter": {
        "and": {
          "filters": [
            {
              "range": {
                "salaryMin": {
                  "from": 250
                }
              }
            },
            {
              "range": {
                "salaryMax": {
                  "from": 250
                }
              }
            },
            {
              "range": {
                "expiredDate": {
                  "from": "now"
                }
              }
            }
          ]
        }
      },
      "aggs": {
        "avg_salary_max": {
          "avg": {
            "field": "salaryMax"
          }
        }
      }
    },
    "top_companies": {
      "filter": {
        "range": {
          "expiredDate": {
            "from": "now"
          }
        }
      },
      "aggs": {
        "top_companies": {
          "terms": {
            "field": "companyId"
          }
        }
      }
    },
    "Web_Services_one_year_analytics": {
      "filter": {
        "fquery": {
          "query": {
            "bool": {
              "must": [
                {
                  "multi_match": {
                    "query": "Java Web Services",
                    "fields": [
                      "skillExperience",
                      "jobDescription",
                      "skillExperience"
                    ],
                    "operator": "and"
                  }
                },
                {
                  "range": {
                    "expiredDate": {
                      "from": "now-372d"
                    }
                  }
                }
              ]
            }
          }
        }
      },
      "aggs": {
        "spring_m": {
          "date_histogram": {
            "field": "approvedDate",
            "format": "yyyy-MM-dd",
            "interval": "month"
          }
        }
      }
    },
    "total_job": {
      "filter": {
        "range": {
          "expiredDate": {
            "from": "now"
          }
        }
      },
      "aggs": {
        "total_job": {
          "value_count": {
            "field": "jobId"
          }
        }
      }
    }
  }
}