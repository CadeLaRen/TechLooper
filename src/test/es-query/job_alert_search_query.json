GET techlooper/job/_search
{
  "query": {
    "filtered": {
      "query": {
        "bool": {
          "must": [
            {
              "match": {
                "location": {
                  "query" : "Ha Noi",
                  "operator": "and"
                }
              }
            }
          ],
          "must": [
            {
              "multi_match": {
                "query": "Java Architect",
                "fields": [
                  "jobTitle",
                  "company"
                ]
              }
            }
          ]
        }
      },
      "filter": {
        "range": {
          "createdDateTime": {
            "from": "now-30d/d"
          }
        }
      }
    }
  },
  "sort": [
    {
      "topPriority": {
        "order": "desc"
      }
    },
    {
      "_score": {
        "order": "desc"
      }
    },
    {
      "createdDateTime": {
        "order": "desc"
      }
    }
  ]
}
